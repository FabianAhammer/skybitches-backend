{
  "name": "skybitches-backend",
  "version": "1.0.0",
  "description": "Expressjs backend for Skybitches",
  "main": "index.js",
  "scripts": {
    "dev": "npx tsc && npm run copy-files && npm run start-db && concurrently \"npx tsc --watch\" \"nodemon --ignore 'spoaherd*.*' -q dist/backend/index.js\" ",
    "prod": "concurrently \"npx tsc --watch\" \"nodemon -q dist/backend/index.js\" ",
    "start-db": "docker-compose --env-file .dev-env up -d",
    "buildWeb": "npx tsc && npm run copy-files",
    "build": "echo 'No build found!'",
    "copy-files": "cp ./utility/spoaherd.sh ./dist/backend/utility/"
  },
  "author": "AFA",
  "license": "ISC",
  "dependencies": {
    "@types/amqplib": "^0.10.1",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "mongodb": "^5.7.0",
    "node-cron": "^3.0.2",
    "shelljs": "^0.8.5",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/node": "^20.4.2",
    "@types/node-cron": "^3.0.10",
    "@types/shelljs": "^0.8.14",
    "@types/ws": "^8.5.5",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
